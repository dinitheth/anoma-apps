module Logic;

import Stdlib.Prelude open;
import Applib open;

--- A logic function that is always valid.
logic : Logic := Logic.alwaysTrue;

type LogicWrapper :=
  mk@{
    logic : Logic;
  };

--- Compute the logic function for the HelloWorld ;Resource;
main : LogicWrapper :=
  LogicWrapper.mk@{
    logic;
  };
